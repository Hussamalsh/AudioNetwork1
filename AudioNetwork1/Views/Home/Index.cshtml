@model TrackIndexModel

@foreach (var t in Model.Tracks)
{
    @Html.Partial("TrackSummary", new TrackComposerViewModel{Track = t, Composer =  Model.Composers.Where(x => x.Id == t.Id).FirstOrDefault()})
}

<div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-default"
     page-class-selected="btn-primary" page-url-category="@Model.CurrentCategory" class="btn-group pull-right">
</div>